l>\r\n                  </div>\r\n                  <!-- radio 2 -->\r\n                  <div class=\"form-check\">\r\n                    <input class=\"form-check-input\" type=\"radio\" id=\"kelapaSawit\" value=\"10\" v-model=\"komoditas\" />\r\n                    <label for=\"kelapaSawit\" class=\"form-check-label\">Kelapa Sawit</label>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div class=\"row mb-4\">\r\n              <div class=\"col-12\">\r\n                <input type=\"text\" placeholder=\"Tahun - Jika lebih dari 1 berikan tanda , (koma)\" v-model=\"tahun\" class=\"form-control\">\r\n              </div>\r\n            </div>\r\n          </div>\r\n        <div>\r\n          <div @click=\"getdata()\" class=\"btn w-100 mb-4 btn-primary\">get data</div>\r\n        </div>\r\n        <table class=\"table table-bordered\">\r\n          <thead class=\"thead\">\r\n            <tr>\r\n              <th>RG Code</th>\r\n              <th>Negara Asal</th>\r\n              <th>Tipe Transaksi</th>\r\n              <th>Negara Tujuan</th>\r\n              <th>Tahun</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody class=\"tbody\" v-if=\"sic.length > 0\">\r\n            <tr v-for=\"data in sic\" v-bind:key=\"data.rgCode\">\r\n              <td>{{ data.rgCode }}</td>\r\n              <td>{{ data.rtTitle }}</td>\r\n              <td>{{ data.rgDesc }}</td>\r\n              <td>{{ data.ptTitle }}</td>\r\n              <td>{{ data.yr }}</td>\r\n            </tr>\r\n          </tbody>\r\n          <tbody class=\"tbody\" v-else>\r\n            <tr>\r\n              <td colspan=\"5\" class=\"text-center\">Data tidak ada</td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n        <div v-if=\"this.banding == true\">\r\n          <div @click=\"getBanding()\" class=\"btn w-100 mb-4 btn-warning\">Bandingkan Sekarang</div>\r\n        </div>\r\n        <table v-if=\"this.banding == true\" class=\"table table-bordered\">\r\n          <thead class=\"thead\">\r\n            <tr>\r\n              <th>RG Code</th>\r\n              <th>Negara Asal</th>\r\n              <th>Tipe Transaksi</th>\r\n              <th>Negara Tujuan</th>\r\n              <th>Tahun</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody class=\"tbody\" v-if=\"sicBanding.length > 0\">\r\n            <tr v-for=\"data in sicBanding\" v-bind:key=\"data.rgCode\">\r\n              <td>{{ data.rgCode }}</td>\r\n              <td>{{ data.rtTitle }}</td>\r\n              <td>{{ data.rgDesc }}</td>\r\n              <td>{{ data.ptTitle }}</td>\r\n              <td>{{ data.yr }}</td>\r\n            </tr>\r\n          </tbody>\r\n          <tbody class=\"tbody\" v-else>\r\n            <tr>\r\n              <td colspan=\"5\" class=\"text-center\">Data tidak ada</td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n      <div class=\"col-12 col-md-4 col-lg-4\">\r\n        <!-- card text detail -->\r\n        <div class=\"card\">\r\n          <div class=\"card-header\">\r\n            Detail Information\r\n          </div>\r\n          <div class=\"card-body px-2 pt-4\">\r\n            <h4 class=\"text-center\">Negara Utama</h4>\r\n            <hr>\r\n            <div class=\"row mx-2\">\r\n              <div class=\"col-12\">\r\n                <div class=\"row d-flex justify-content-between\">\r\n                  <div class=\"col-6\" style=\"font-weight: bold;\">\r\n                    Total Data\r\n                  </div>\r\n                  <div class=\"col-6\" style=\"text-align: right;\">\r\n                    {{ sic.length }}\r\n                  </div>\r\n                </div>\r\n                <div class=\"row d-flex justify-content-between\">\r\n                  <div class=\"col-6\" style=\"font-weight: bold;\">\r\n                    Total Export\r\n                  </div>\r\n                  <div class=\"col-6\" style=\"text-align: right;\">\r\n                    {{ this.export.length }}\r\n                  </div>\r\n                </div>\r\n                <div class=\"row d-flex justify-content-between\">\r\n                  <div class=\"col-6\" style=\"font-weight: bold;\">\r\n                    Total Import\r\n                  </div>\r\n                  <div class=\"col-6\" style=\"text-align: right;\">\r\n                    {{ this.import.length }}\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <h4 class=\"text-center mt-4\">Negara Pebanding</h4>\r\n            <hr>\r\n            <div class=\"row mx-2\">\r\n              <div class=\"col-12\">\r\n                <div class=\"row d-flex justify-content-between\">\r\n                  <div class=\"col-6\" style=\"font-weight: bold;\">\r\n                    Total Data\r\n                  </div>\r\n                  <div class=\"col-6\" style=\"text-align: right;\">\r\n                    {{ sicBanding.length }}\r\n                  </div>\r\n                </div>\r\n                <div class=\"row d-flex justify-content-between\">\r\n                  <div class=\"col-6\" style=\"font-weight: bold;\">\r\n                    Total Export\r\n                  </div>\r\n                  <div class=\"col-6\" style=\"text-align: right;\">\r\n                    {{ this.exportBanding.length }}\r\n                  </div>\r\n                </div>\r\n                <div class=\"row d-flex justify-content-between\">\r\n                  <div class=\"col-6\" style=\"font-weight: bold;\">\r\n                    Total Import\r\n                  </div>\r\n                  <div class=\"col-6\" style=\"text-align: right;\">\r\n                    {{ this.importBanding.length }}\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <!-- card chart detail -->\r\n        <div class=\"card\">\r\n          <div class=\"card-header\">\r\n            Chart Data\r\n          </div>\r\n          <div class=\"card-body\">\r\n            <chart :dataitem=\"sic\"> \r\n\r\n            </chart>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Navbar from '../components/Navbar.vue'\r\nimport axios from 'axios'\r\nimport Chart from '../components/Chart.vue';\r\n\r\nexport default {\r\n  name: 'SIC',\r\n  components: {\r\n    Navbar,\r\n    Chart\r\n  },\r\n  data () {\r\n    return {\r\n      sic: [],\r\n      sicBanding: [],\r\n      export: [],\r\n      import: [],\r\n      exportBanding: [],\r\n      importBanding: [],\r\n      tahun: null ,\r\n      negara: null,\r\n      pebanding: null,\r\n      komoditas: null,\r\n      banding: false\r\n    };\r\n  },\r\n  methods: {\r\n    getdata() {\r\n      this.sicBanding = []\r\n      axios.get(`https://comtrade.un.org/api/get`, {\r\n        params: {\r\n          ps: this.tahun,\r\n          r: this.negara,\r\n          px: 'HS',\r\n          p: this.pebanding,\r\n          cc: this.komoditas,\r\n          max: 500,\r\n          freq: 'A',\r\n          rg: 'all',\r\n          type: 'C'\r\n        }\r\n      }).then(res => {\r\n        this.sic = res.data.dataset\r\n        this.banding = true\r\n        \r\n        // HITUNG EXPORT\r\n        this.export = res.data.dataset.filter(function (item) {\r\n          return item.rgDesc.match(\"Export\")\r\n        })\r\n\r\n        // HITUNG IMPORT\r\n        this.import = res.data.dataset.filter(function (item) {\r\n          return item.rgDesc.match(\"Import\")\r\n        })\r\n        \r\n        return this.sic\r\n        }).catch(err => console.log(err));\r\n    },\r\n    getBanding() {\r\n      axios.get(`https://comtrade.un.org/api/get`, {\r\n        params: {\r\n          ps: this.tahun,\r\n          r: this.pebanding,\r\n          px: 'HS',\r\n          p: this.negara,\r\n          cc: this.komoditas,\r\n          max: 500,\r\n          freq: 'A',\r\n          rg: 'all',\r\n          type: 'C'\r\n        }\r\n      }).then(res => {\r\n        this.sicBanding = res.data.dataset\r\n        \r\n        // HITUNG EXPORT\r\n        this.exportBanding = res.data.dataset.filter(function (item) {\r\n          return item.rgDesc.match(\"Export\")\r\n        })\r\n\r\n        // HITUNG IMPORT\r\n        this.importBanding = res.data.dataset.filter(function (item) {\r\n          return item.rgDesc.match(\"Import\")\r\n        })\r\n        \r\n        return this.sic\r\n        }).catch(err => console.log(err));\r\n    }\r\n  },\r\n  mounted() {\r\n  }\r\n};\r\n</script>\r\n"]}]}